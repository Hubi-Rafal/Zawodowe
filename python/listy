print("*********** Zadanie 4 ***********")
def Zadanie5(n):
  list5 = []
  for i in range(1,n):
    if n%i==0:
      list5.append(i)
  print(list5)

n = int(input("Podaj 1 liczbe: "))
Zadanie5(n)
